{

  "manifest_version": 2,

  "name": "Positionable!",
  "description": "An extension for spriting and positioning elements.",
  "version": "1.0",

  "icons": {
    "16":  "icons/16.png",
    "32":  "icons/32.png",
    "48":  "icons/48.png",
    "128": "icons/128.png"
  },

  "permissions": [
    "tabs", "http://*/*"
  ],

  "background": {
    "persistent":false,
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/16.png",
    "default_title": "Chrome Extension"
  },

  "content_scripts": [
    {
    "matches": ["http://www.google.com/*"],
    "js": ["position.js"]
    }
  ],


  "web_accessible_resources": [
    "fonts/icons.eot",
    "fonts/icons.svg",
    "fonts/icons.ttf",
    "fonts/icons.woff"
  ]

}
